{{#if active}}
    {{#unless without_text}}
        <div class="about section text-capitalize">My {{type}}</div>
    {{/unless}}
{{else}}
    <div class="description">
        {{stacked-icon value="fa-exclamation" icon="fa-circle" size="1" class="d-inline"}} Select {{type}} {{text}} 
    </div>
{{/if}}
