{{title "Roadmap"}}

<div class="container-fluid pl-0 pr-0">
    {{develop-carousel}}
    {{#link-to "me.overview" class="exit"}}Home {{stacked-icon value="fa-home" icon="fa-circle" size="1" class="d-inline"}}{{/link-to}}
    {{develop-roadmap-overview class="explain text-center hidden-xs-down shadow"}}
    {{develop-roadmap-overview class="explain text-left hidden-sm-up shadow"}}
    <main class="col-sm-12 col-md-12 offset-sm-0">
        <div class="row main">
            <div class="col-md-12 col-sm-12">
                {{develop-roadmap-content jobs=model.jobs projects=model.projects params=model.params teams=model.teams refresh="refresh" class="row"}}
            </div>
        </div>
    </main>
    {{application-foot class="foot-hold text-center"}}
</div>
