<div class="full">
    <h3>Leads Generated</h3>
    <div class="about section">Profiles that {{team.name}} at {{team.company}} has shared with you</div>

    {{#each shares as |user|}}
        <div class="list-group-item rounded">
            <h4 class="line">
                {{#link-to "profile" user.share_profile.id}}{{user.share_first_name}}{{/link-to}} {{#if user.share_profile.title}} - {{user-profile-descriptor user_profile=user.share_profile class="d-inline"}}{{/if}}
            </h4>
            <small class="line text-muted">                    
                Shared by {{#link-to "profile" user.sender_id}}{{user.sender_first_name}}{{/link-to}} {{moment-from-now user.created_at}}
            </small>
        </div>
    {{/each}}
</div>
