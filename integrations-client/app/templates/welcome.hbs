{{title "Welcome"}}
<div class="container-fluid pl-0 pr-0">
    <main class="col-sm-12 col-md-12 offset-sm-0 pl-0 pr-0">
        <div class="row main">
            <div class="col-md-6 col-sm-12 offset-md-0 slide hidden-sm-down welcome full">{{user-welcome-overview params=model.params states=model.states user=model.user teams=model.teams skillsets=model.skillsets roles=model.roles refresh="refresh"}}</div>
            <div class="col-md-6 col-sm-12 offset-md-0 slide hidden-md-up welcome full">{{user-welcome-overview params=model.params states=model.states user=model.user teams=model.teams skillsets=model.skillsets roles=model.roles refresh="refresh"}}</div>
            <div class="col-md-6 col-sm-12 offset-md-6 still hidden-sm-down welcome">{{user-welcome-content params=model.params states=model.states teams=model.teams roles=model.roles}}</div>
            <div class="col-md-6 col-sm-12 offset-md-6 still hidden-md-up welcome">{{user-welcome-content params=model.params states=model.states teams=model.teams roles=model.roles}}</div>
        </div>
    </main>
</div>
