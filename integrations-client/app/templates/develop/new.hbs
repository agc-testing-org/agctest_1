{{title "New Project"}}
<div class="container-fluid pl-0 pr-0">
    <nav class="col-md-1 bg-faded sidebar hidden-sm-down">
        <ul class="nav nav-pills flex-column timeline">
            <div class="menu">
                {{#link-to "me" tagName='li' href=false class="cursor-pointer nav-item text-center text-capitalize" }}
                    <i class="fa fa-fw fa-user"></i>My Profile
                {{/link-to}}
            </div>
            {{application-foot}}
        </ul>
    </nav>
    <nav class="col-md-1 bg-faded sidebar hidden-md-up">
        <ul class="nav nav-pills flex-column timeline">
            <div class="menu">
                {{#link-to "me" tagName='li' href=false class="cursor-pointer nav-item text-center text-capitalize" }}
                    <i class="fa fa-fw fa-user"></i>My Profile
                {{/link-to}}
            </div>
            {{application-foot}}
        </ul>
    </nav>
    <main class="col-sm-12 col-md-12 offset-sm-0 pl-0 pr-0">
        <div class="row main">
            <div class="col-md-5 col-sm-12 offset-md-0 slide hidden-sm-down">
                {{develop-create-overview}}
            </div>
            <div class="col-md-5 col-sm-12 offset-md-0 slide hidden-md-up">
                {{develop-create-overview}}
            </div>
            <div class="col-md-6 col-sm-12 offset-md-6 still hidden-sm-down">{{develop-create-content repositories=model.repositories refresh="refresh"}}</div>
            <div class="col-md-6 col-sm-12 offset-md-6 still hidden-md-up">{{develop-create-content repositories=model.repositories refresh="refresh"}}</div>
        </div>
    </main>
</div>
