<ul class="list-group">
    {{#each user_teams as |user|}}
        <li class="list-group-item">
            {{#if user.user_first_name}}
                {{user.user_first_name}}
            {{else}}
                {{user.user_email}}
            {{/if}}

            {{user.user_last_name}} invited by {{user.sender_first_name}} {{user.sender_last_name}}
        </li>
    {{/each}}
</ul>
